    "dsl",
    "generator",
    "rendering",
    "ui",
    "ydant"
  ],
  "homepage": "https://github.com/cwd-k2/ydant#readme",
  "bugs": {
    "url": "https://github.com/cwd-k2/ydant/issues"
  },
  "license": "MIT",
  "author": "cwd-k2",
  "repository": {
    "type": "git",
    "url": "https://github.com/cwd-k2/ydant.git",
    "directory": "packages/core"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
